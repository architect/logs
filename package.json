{
  "name": "@architect/logs",
  "version": "4.0.2",
  "description": "Manage logging from Architect-provisioned cloud functions",
  "main": "src/index.js",
  "bin": {
    "arc-logs": "src/cli.js"
  },
  "scripts": {
    "test": "npm run lint && npm run coverage",
    "test:unit": "cross-env tape 'test/**/*-tests.js' | tap-spec",
    "coverage": "nyc --reporter=lcov --reporter=text npm run test:unit",
    "lint": "eslint . --fix",
    "rc": "npm version prerelease --preid RC"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/architect/logs.git"
  },
  "keywords": [
    "arc",
    "logs"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/architect/logs/issues"
  },
  "homepage": "https://github.com/architect/logs#readme",
  "files": [
    "src/*"
  ],
  "dependencies": {
    "@architect/inventory": "~3.1.1",
    "@architect/utils": "~3.1.1",
    "aws-sdk": "2.1001.0",
    "chalk": "4.1.2",
    "minimist": "~1.2.6",
    "run-parallel": "~1.2.0",
    "run-waterfall": "~1.1.7",
    "strftime": "~0.10.1"
  },
  "devDependencies": {
    "@architect/eslint-config": "~2.0.1",
    "aws-sdk-mock": "~5.6.2",
    "cross-env": "~7.0.3",
    "eslint": "^8.15.0",
    "mock-fs": "~5.1.2",
    "mock-require": "~3.0.3",
    "nyc": "^15.1.0",
    "sinon": "^13.0.1",
    "tap-spec": "^5.0.0",
    "tape": "^5.5.3"
  },
  "eslintConfig": {
    "extends": "@architect/eslint-config"
  }
}

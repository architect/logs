{
  "name": "@architect/logs",
  "version": "6.0.0",
  "description": "Manage logging from Architect-provisioned cloud functions",
  "main": "src/index.js",
  "bin": {
    "arc-logs": "src/cli.js"
  },
  "scripts": {
    "test": "npm run lint && npm run coverage",
    "test:nolint": "npm run coverage",
    "test:unit": "cross-env tape 'test/**/*-tests.js'",
    "coverage": "nyc --reporter=lcov --reporter=text npm run test:unit",
    "lint": "eslint . --fix",
    "rc": "npm version prerelease --preid RC"
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/architect/logs.git"
  },
  "keywords": [
    "arc",
    "logs"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/architect/logs/issues"
  },
  "homepage": "https://github.com/architect/logs#readme",
  "files": [
    "src/*"
  ],
  "dependencies": {
    "@architect/inventory": "~5.0.0",
    "@architect/utils": "~5.0.0",
    "@aws-lite/client": "~0.23.2",
    "@aws-lite/cloudformation": "~0.1.3",
    "@aws-lite/cloudwatch-logs": "~0.1.0",
    "strftime": "0.10.3"
  },
  "devDependencies": {
    "@architect/eslint-config": "~3.0.0",
    "cross-env": "~10.0.0",
    "eslint": "~9.36.0",
    "mock-require": "~3.0.3",
    "mock-tmp": "~0.0.4",
    "nyc": "~17.1.0",
    "sinon": "~21.0.0",
    "tape": "~5.9.0"
  }
}
